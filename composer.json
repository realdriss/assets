{
    "name": "realdriss/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "RealDriss",
        "RealDriss cms",
        "RealDriss platform",
        "RealDriss assets"
    ],
    "homepage": "https://RealDriss.com",
    "authors": [
        {
            "name": "Sang Nguyen",
            "email": "sangnguyenplus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "laravel/framework": "^5.6|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RealDriss\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RealDriss\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealDriss\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "RealDriss\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
